	<parameter name="e_No" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT projects.project_name, projects.project_number,
       projects_contracts.contract_owner,
       NVL(projects_contracts.contract_duration,0) contract_duration,
       projects_contracts.contract_duration_unit,
       projects_contracts.contract_site_recieve_date,
       projects_extracts.extract_number,
(select wrk_dept.DEPT_NAME from arc_users  ,wrk_dept  where arc_users.DEPT_ID = wrk_dept.Id and user_id = projects_extracts.EXTRACT_CREATED_BY) dept,
       projects_extracts.extract_spent_time_percentage,
       projects_extracts.extract_finish_percentage,
       projects_extracts.extract_to, projects_contracts.contract_value extract_value ,
'بيانات مستخلص لمشروع '||projects.project_name||'رقم المستخلص '||projects_extracts.extract_number label_head_text,
'سعادة ' || PROJECTS_EXTRACTS.EXTRACT_TO ||' المحترم  ' extract_to_txt ,
'نرفق لسعادتكم المستخلص ('||projects_extracts.extract_number
||
') للمشروع المذكور بياناته أعلاه بمبلغ مستحق للمقاول صرفه و قدره  ريــ  '
 ||projects_extracts.extract_value ||
 ' ــال ,بالاضافة الى مبلغ القيمة المضافة وقدره  فقط     '||
 extract_value * .05

 ||
  NUM_AR(projects_extracts.extract_value) ||chr(10)||
  'نأمل بعد الإطلاع الإيعاز لمن يلزم بتدقيق المستخلص و إكمال اللازم حسب الأنظمة و التعليمات مع التـأكد من وجود تنازل بنكي . '
  long_txt ,
(select HRS_SIGNS.SIGN_JOB_NAME
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 3 ) SIGN_JOB_NAME  ,
(select GET_REAL_EMPNAME(SIGN_USER_ID)
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 3) sign_real_name ,
(
select WRK_PROFILE_SIGN.DDD
from ARC_RECORDS,HRS_SIGNS ,wrk_profile_sign
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.SIGN_USER_ID  =  wrk_profile_sign.USER_ID
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 3) SIGN001 ,
(select HRS_SIGNS.SIGN_JOB_NAME
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 2) mark_name ,
(select WRK_PROFILE.SIGN2
from ARC_RECORDS,HRS_SIGNS ,wrk_profile
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.SIGN_USER_ID  =  wrk_profile.USER_ID
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 2) mark001 ,
(select ARC_RECORDS.INCOME_NO
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.DOC_id = $P{e_No}
and HRS_SIGNS.sign_step_id = 2) income_no ,
(select ARC_RECORDS.REC_H_DATE
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 236
and ARC_RECORDS.id = HRS_SIGNS.arc_record_id
and HRS_SIGNS.DOC_id =  $P{e_No}
and HRS_SIGNS.sign_step_id = 2) h_date,
PROJECTS_EXTRACTS.EXTRACT_VALUE as total
  FROM projects_extracts, projects_contracts, projects
 WHERE (    (projects_contracts.contract_id = projects_extracts.contract_id)
        AND (projects.project_id = projects_contracts.project_id)
       )
       and EXTRACT_ID = $P{e_No}]]>
	</queryString>
