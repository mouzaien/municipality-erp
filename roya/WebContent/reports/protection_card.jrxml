	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="DataAdapter1.xml"/>
	<parameter name="artId" class="java.lang.Integer"/>
	<parameter name="userId" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
reqs.artCode code ,
reqs.artName name ,
 reqs.EMPNAME usrName, 
 reqs.DEPT_NAME deptName ,
reqs.UnitName itemUnit ,
reqs.strName strName ,
NVL(reqs.NOTES, '')  notes ,
 reqs.srfDate sarfDate ,
reqs.SERIALNUMBER EXSPECIAL_NO ,
TRANSFER_OWNERSHIP.QTY transferQ , 
( reqs.srfQ  - NVL(RETURN_STORE_DETAILS.QTY, 0) - NVL(TRANSFER_OWNERSHIP.QTY, 0) )   a3ohad ,
RETURN_STORE_DETAILS.QTY RETURN_QTY ,
reqs.srfQ qtySarf ,
RETURN_STORE.RET_HDATE R_datetransaction ,
RETURN_STORE.SERIAL_NUMBER R_SPECIAL_NO
from
RETURN_STORE_DETAILS , (SELECT * from RETURN_STORE WHERE nvl(RETURN_STORE.STATUS_Y_N ,'N')='Y') RETURN_STORE, (SELECT * from TRANSFER_OWNERSHIP WHERE nvl(TRANSFER_OWNERSHIP.STATUS ,'N')='Y')TRANSFER_OWNERSHIP,
(

SELECT
	TDR_INV_GNRL_REQ.INV_G_R_NO,
	NVL(SERIALNUMBER, 0)SERIALNUMBER,
	INV_G_R_DATE srfDate,
	NOTES,
	EMPNAME ,
	NVL(ACCEPT_Y_N, 'P')status ,
	art.NAME artName ,
	art.code  artCode ,
	DEPT_NAME,
    reqdet.INIT_COUNT srfQ ,
    TDR_ITEMUNIT.ITEMUNITNAME UnitName,
    WHS_WAREHOUSES.STRNAME strName
    

FROM
	TDR_INV_GNRL_REQ,
	ARC_USERS ,
	article art,
	WHS_WAREHOUSES ,
	TDR_ITEMUNIT,
	WRK_DEPT ,
	(
	SELECT
		DISTINCT s.doc_id id
	FROM
		arc_records r,
		hrs_signs s,
		tdr_inv_gnrl_req rq
	WHERE
		rq.INV_G_R_NO = doc_id
		AND r.id = s.arc_record_id
		AND r.app_type = 237
		-- app_type (sarf,....)
		AND s.sign_step_id = 1 )ids,
	(
	SELECT
		INV_G_R_NO, INIT_COUNT ,
		MIN(itemid)itemid
	FROM
		TDR_INV_REQ_DET
	GROUP BY
		INV_G_R_NO , INIT_COUNT)reqdet
WHERE
	reqdet.INV_G_R_NO = ids.id
	AND reqdet.itemid = art.ID
	AND (art.ID =$P{artId} OR $P{artId} = -1)
	AND WHS_WAREHOUSES.STRNO =TDR_INV_GNRL_REQ.STORE_NUMBER
	AND  art.ITEM_UNITE_ID= TDR_ITEMUNIT.ITEMUNITID  
	AND art.ART_TYPE = 2 	
	AND ARC_USERS.DEPT_ID = WRK_DEPT.ID
	AND (ARC_USERS.USER_ID = $P{userId} OR $P{userId} = -1 )
	AND ids.id = TDR_INV_GNRL_REQ.INV_G_R_NO
	AND TDR_INV_GNRL_REQ.USERID = ARC_USERS.USER_ID
	AND TDR_INV_GNRL_REQ.ACCEPT_Y_N ='Y'
 )reqs
where
reqs.INV_G_R_NO = RETURN_STORE_DETAILS.EXCH_MASTER_ID (+) 
AND RETURN_STORE.ID(+) = RETURN_STORE_DETAILS.RET_ID 
AND reqs.INV_G_R_NO = TRANSFER_OWNERSHIP.EXCH_MASTER_ID (+)

UNION

	SELECT
	ts.ART_CODE code ,
	ar.NAME name ,
	usr.EMPNAME usrName ,
	DEPT_NAME deptName ,
	un.ITEMUNITNAME itemUnit ,
	WHS_WAREHOUSES.STRNAME strName ,
	
	NVL(ts.NOTES, '') notes ,
	master.INV_G_R_DATE  sarfDate,
    
   
	master.SERIALNUMBER EXSPECIAL_NO ,
	1 transferQ,
	TO_NUMBER(1 - NVL(RETURN_STORE_DETAILS.QTY, 0)) a3ohad ,
	NVL(RETURN_STORE_DETAILS.QTY, 0)  RETURN_QTY ,
	0 qtySarf ,
     RETURN_STORE.RET_HDATE R_datetransaction ,
    RETURN_STORE.SERIAL_NUMBER R_SPECIAL_NO 
	FROM TRANSFER_OWNERSHIP ts ,article ar,
	TDR_ITEMUNIT un ,TDR_INV_GNRL_REQ master , ARC_USERS usr ,WRK_DEPT
	, WHS_WAREHOUSES  ,RETURN_STORE_DETAILS , (SELECT * from RETURN_STORE WHERE nvl(RETURN_STORE.STATUS_Y_N ,'N')='Y') RETURN_STORE
	WHERE 
	ts.TO_USER = $P{userId}
	AND ts.EXCH_MASTER_ID = RETURN_STORE_DETAILS.EXCH_MASTER_ID (+) 
    AND RETURN_STORE.ID(+) = RETURN_STORE_DETAILS.RET_ID
	AND ts.STATUS = 'Y'
	AND ts.ART_ID  = ar.ID 
	AND un.ITEMUNITID  = ar.ITEM_UNITE_ID 
	AND ts.EXCH_MASTER_ID (+) = master.INV_G_R_NO
	AND ts.TO_USER  = usr.USER_ID
    AND usr.DEPT_ID = WRK_DEPT.ID
    AND WHS_WAREHOUSES.STRNO =ts.STR_NO]]>
	</queryString>
