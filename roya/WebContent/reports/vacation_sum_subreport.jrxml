	<queryString>
		<![CDATA[SELECT ALL HRSV$INF001.EMPNO,
fun_get_inf001_ar(HRSV$INF001.EMPNO) NM,
HRSV$INF001.RANKCOD,HRMS_GET_GOVJOB4(HRSV$INF001.JOBCOD)JBNM, get_hrs_dept_name(HRSV$INF001.DEPTCOD) DPTNM,
HRS_EMP_HISTORICAL.JOBNO,HRS_MASTER_FILE.FAPPLDAT,
HRS_VAC_SOLD.VAC_OLD_SOLD + HRS_VAC_SOLD.VAC_CURR_YEAR_SOLD ALLCREDIT,
nvl(HRS_GET_SUM_VACA(HRS_EMP_HISTORICAL.EMPNO,116),0) VACSUM,
(HRS_VAC_SOLD.VAC_OLD_SOLD + HRS_VAC_SOLD.VAC_CURR_YEAR_SOLD - nvl(HRS_GET_SUM_VACA(HRS_EMP_HISTORICAL.EMPNO,116),0)) VACRSLT,
(SELECT PR.SIGN2 FROM WRK_PROFILE PR WHERE PR.USER_ID = GET_SYS_PROP_VAL('HR_ID')) HRPRSIGN,
(SELECT PR.SIGN2 FROM WRK_PROFILE PR WHERE PR.USER_ID = GET_SYS_PROP_VAL('VAC_SIGN_RESPONSIBLE')) VACRESPSIGN,
note N,
HRS_EMP_VCTON.EMPNO, HRS_EMP_VCTON.VACATYP, HRS_EMP_VCTON.VACAPRD, HRS_EMP_VCTON.VACASTRT,
HRS_EMP_VCTON.VACAEND, HRS_EMP_VCTON.EXCNO, HRS_EMP_VCTON.EXCDT,
HRS_EMP_VCTON.VACWRKFROM, HRS_EMP_VCTON.VACWRKTO, SYS052.NAME


FROM HRSV$INF001 , HRS_EMP_HISTORICAL , HRS_MASTER_FILE, HRS_EMP_VCTON, SYS052, HRS_VAC_SOLD--, USER_VAC_SOLD

WHERE HRS_EMP_HISTORICAL.EMPNO = HRSV$INF001.EMPNO AND  HRS_MASTER_FILE.EMPNO = HRS_VAC_SOLD.EMPNO
AND HRS_MASTER_FILE.EMPNO = HRSV$INF001.EMPNO
AND HRS_MASTER_FILE.EMPNO  = $P{pempno} 
and flg=1 
and (HRS_EMP_VCTON.VACATYP = SYS052.ID)
and HRS_EMP_VCTON.EMPNO = HRS_MASTER_FILE.EMPNO
and VACATYP= $P{vactype} 
and VAC_ACCPT_Y_N = 'Y'
order by to_number(substr(VACWRKFROM,-4) || substr(VACWRKFROM,4,2)|| substr(VACWRKFROM,1,2))]]>
	</queryString>
