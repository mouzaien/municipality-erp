	<parameter name="street" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
	rownum num,
	INV_SITES.SITE_NAME SITE,
	INV_CONT_TYPES.CONT_TYPE_DESC ACTIVITY_TYPE,
	STREET,
	LIC_DISTRICT.NAME DISTRICT_NAME,
	BUILDING_TYPE.NAME BUILDING_NAME,
	COMPONENTS
FROM
	REAL_ESTATE,
	INV_SITES,
	INV_CONT_TYPES ,
	LIC_DISTRICT,
	BUILDING_TYPE
WHERE
	REAL_ESTATE.SITE_LYPE_ID = INV_SITES.ID
	AND REAL_ESTATE.ACTIVITY_TYPE_ID = INV_CONT_TYPES.ID
	AND REAL_ESTATE.DISTRICT = LIC_DISTRICT.ID
	AND REAL_ESTATE.BUILD_TYPE = BUILDING_TYPE.ID
	AND( ACTIVITY_TYPE_ID =$P{activityId} OR $P{activityId} =-1)
	AND (SITE_LYPE_ID = $P{siteId} OR $P{siteId}= -1) 
	AND (COMPONENTS = $P{components}  OR $P{components} = '-1')
	AND( BUILD_TYPE =$P{buildId}  OR $P{buildId} = -1)
	AND (DISTRICT =$P{districtId} OR $P{districtId} =-1)
	AND (STREET = $P{street}  OR $P{street} = '-1')]]>
	</queryString>
