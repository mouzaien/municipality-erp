	<queryString>
		<![CDATA[select step_id,
wrk_get_confidential(convert_comm(id,step_id),id,step_id) as comm
,get_purpose(app_purp) purp,

reverse_hdate(h_date) || '     '   ||  substr(create_time,-2) ||':' ||substr(create_time,4,2)||':' ||substr(create_time,1,2) || '  ' ||
replace(replace(to_char(create_date,'AM'),'AM','صباحا'),'PM','مساءا')
as create_time,
reverse_hdate(h_date) || '     '   ||  replace(replace(to_char(create_date,'AM'),'AM','AM'),'PM','PM') as ctime ,


get_name(from_id) ||' - ' ||  get_real_empname(from_id)   fr,
get_name(TO_ID) || ' - ' ||  get_real_empname(to_id)   tt

from wrk_application
where id = $P{p1}
  order by step_id]]>
	</queryString>
