	<queryString>
		<![CDATA[select fun_get_inf001_ar (arc_users.empno) as employer,natno,date_medical,HRS_MEDICAL_CHECK.PLACE_MEDICAL,
REC_H_DATE APP_DATE,
income_no APP_income_no,
(select SIGN_USER_ID from  HRS_SIGNS where HRS_SIGNS.SIGN_STEP_ID=2 and ARC_RECORD_ID = arc.id) mark,
(select WRK_PROFILE.SIGN2 from  HRS_SIGNS,WRK_PROFILE where HRS_SIGNS.SIGN_STEP_ID=2 and HRS_SIGNS.SIGN_USER_ID = WRK_PROFILE.USER_ID and ARC_RECORD_ID = arc.id) mark_img,
