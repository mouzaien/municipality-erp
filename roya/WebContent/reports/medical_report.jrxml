from HRS_MEDICAL_CHECK ,arc_users,hrs_master_file,WRK_PROFILE_SIGN,HRS_SIGNS,arc_records arc

where HRS_MEDICAL_CHECK.DEMAND_USER_ID = ARC_USERS.USER_ID
 and WRK_PROFILE_SIGN.USER_ID = HRS_SIGNS.SIGN_USER_ID
and  HRS_MASTER_FILE.EMPNO = ARC_USERS.EMPNO
and HRS_MEDICAL_CHECK.ACCEPT_Y_N = 'Y'
 and HRS_SIGNS.ARC_RECORD_ID=260706
  and HRS_SIGNS.SIGN_IS_VISIBLE = 1
  and ARC.ID= HRS_SIGNS.ARC_RECORD_ID
  and HRS_SIGNS.DOC_ID=HRS_MEDICAL_CHECK.ID]]>
	</queryString>
