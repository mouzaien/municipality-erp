	<queryString>
		<![CDATA[/* Formatted on 2016/11/22 09:41 (Formatter Plus v4.8.8) */
SELECT ALL lic_helth_data.ID a1,
           lic_helth_data.appl_date,
            lic_helth_data.apll_owner
           || ' تاريخه: '
           || lic_helth_data.id_date
           || ' هـ'
           || ' مصدره :'
           || lic_helth_data.id_source a5,
            lic_helth_data.appl_name a2,
           lic_helth_data.id_date, lic_helth_data.is_type,
              'الجنسيــة : '
           || lic_helth_data.nation
           || ' '
           || ' المهنة :'
           || lic_helth_data.job a3,lic_helth_data.job,lic_helth_data.nation,
           lic_helth_data.station,
           '' || lic_helth_data.rsl_end_date || ' هــ ' d2,
           '' || lic_helth_data.rsl_bgn_date || ' هــ ' d1,
              'المهنـــــة : '
           || lic_helth_data.job
           || ' لدى '
           || (SELECT trd_name || ' ' || lic_address
                 FROM lic_master_helth
                WHERE appl_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )  ) a4,
(SELECT trd_name || ' ' || lic_address
                 FROM lic_master_helth
                WHERE appl_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )  ) a4_1,

(SELECT trd_name
                 FROM lic_master_helth
                WHERE appl_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )  ) trd_name,
(SELECT  lic_address
                 FROM lic_master_helth
                WHERE appl_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} ) ) lic_address,

           lic_helth_data.apll_owner, lic_helth_data.tefod_date,
           lic_helth_data.homma_date, lic_helth_data.k_name,
           lic_helth_data.date_depend, lic_helth_data.depend1,
           '*' || lic_helth_data.ID || '*' barcode, lic_helth_data.ID,
           lic_master_helth.apll_type ,
           (SELECT ALL  ARC_PEOPLE_PIC.PIC FROM  ARC_PEOPLE_PIC where ARC_PEOPLE_PIC.natno=lic_helth_data.apll_owner ) PIC ,
           ' تم إدخال البيانات بواسطة  '||NVL(get_name(lic_master_helth.DEPEND1),'**') createdBY ,

(select HRS_SIGNS.SIGN_date
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 117
and ARC_RECORDS.id = $P{p1}
and HRS_SIGNS.DOC_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )
and HRS_SIGNS.sign_step_id = 2 ) SIGN_DATE  ,
(select HRS_SIGNS.SIGN_JOB_NAME
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 117
and ARC_RECORDS.id = $P{p1}
and HRS_SIGNS.DOC_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )
and HRS_SIGNS.sign_step_id = 2 ) SIGN_JOB_NAME  ,

(select GET_REAL_EMPNAME(SIGN_USER_ID)
from ARC_RECORDS,HRS_SIGNS
where APP_TYPE = 117
and ARC_RECORDS.id = $P{p1}
and HRS_SIGNS.DOC_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )
and HRS_SIGNS.sign_step_id = 2) sign_real_name ,


(
select WRK_PROFILE_SIGN.DDD
from ARC_RECORDS,HRS_SIGNS ,wrk_profile_sign
where APP_TYPE = 117
and ARC_RECORDS.id = $P{p1}
and HRS_SIGNS.SIGN_USER_ID  =  wrk_profile_sign.USER_ID
and HRS_SIGNS.DOC_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )
and HRS_SIGNS.sign_step_id = 2) SIGN001







      FROM lic_helth_data, lic_master_helth
     WHERE lic_helth_data.appl_id = lic_master_helth.appl_id
       AND lic_helth_data.appl_id = (select NVL(max(doc_id),0) from hRS_SIGNS where ARC_RECORD_ID =$P{p1} )]]>
	</queryString>
