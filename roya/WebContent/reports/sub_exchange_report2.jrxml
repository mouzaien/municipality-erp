	<queryString>
		<![CDATA[select num, a.code, a.name, a.ITEMUNITNAME, a.init_count, a.count, a.notes, a.SERIALNUMBER
from
(
SELECT rownum num ,ar.code code, ar.name name, un.ITEMUNITNAME ITEMUNITNAME, det.init_count, det.count, det.notes notes, GEN.SERIALNUMBER SERIALNUMBER


FROM  ARTICLE ar, TDR_ITEMUNIT un, TDR_INV_REQ_DET det, TDR_INV_GNRL_REQ gen
where GEN.INV_G_R_NO=DET.INV_G_R_NO and DET.ITEMID=AR.ID and AR.ITEM_UNITE_ID=UN.ITEMUNITID and GEN.INV_G_R_NO=$P{exchange_req_no}

)A,

(
SELECT  level lev
FROM    dual
CONNECT BY
        level <= 11)B

        where b.lev = a.num (+)

        order by b.lev]]>
	</queryString>
