	<queryString>
		<![CDATA[select l.loan_empno,L.LOAN_NAME , D.LOAN_PAID_MONTLY,
F.FST_NAME_AR ||' '||F.SND_NAME_AR||' '||F.TRD_NAME_AR||' '||F.FTH_NAME_AR  as name,
F.NATNO
from hrs_loan l,hrs_loan_details d, hrs_master_file f,hrs_emp_historical his
where L.LOAN_ID=D.LOAN_ID and L.LOAN_EMPNO=F.EMPNO
and D.LOAN_DET_MONTH= $P{month}  and D.LOAN_DET_YEAR= $P{year}
and L.LOAN_TYPE_ID =  $P{loanTypeId}
and HIS.EMPNO = F.EMPNO
and empsts=1
and sal_stp=0
and loan_sts_a_s_e='A'
and serial=HRS_GET_MAX_SERIAL(his.empno)
order by his.catcod asc , RANKCOD desc]]>
	</queryString>
