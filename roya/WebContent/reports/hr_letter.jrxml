HRS_EMP_HISTORICAL.JOBNO ,WRK_PROFILE_SIGN.DDD,SIGN_USER_ID,HRS_LETTER_REQUEST.BANK_ACCOUNT,HRS_LETTER_REQUEST.BANK_ID,
HRS_LETTER_REQUEST.DESTINATION,HRS_LETTER_REQUEST.PURPOSE,HRS_EMP_HISTORICAL.BSCSAL,
GET_REAL_EMPNAME(SIGN_USER_ID) real_name, sign_job_name TITLEPOS,
(SELECT REC_H_DATE FROM ARC_RECORDS WHERE ID =(SELECT MIN(APP_ID) FROM wrk_application WHERE APP_ID = $P{record_id}) ) as APP_DATE,
(SELECT income_no FROM ARC_RECORDS WHERE ID =(SELECT MIN(APP_ID) FROM wrk_application WHERE APP_ID = $P{record_id} ) ) as APP_income_no
from hrs_master_file,hrs_emp_historical,hrs_signs,HRS_LETTER_REQUEST,arc_users,WRK_PROFILE_SIGN
where HRS_MASTER_FILE.EMPNO = HRS_EMP_HISTORICAL.EMPNO
 and  HRS_EMP_HISTORICAL.FLG=1
 and HRS_LETTER_REQUEST.EMPNO = HRS_MASTER_FILE.EMPNO
 and HRS_LETTER_REQUEST.ID = HRS_SIGNS.DOC_ID
 and HRS_SIGNS.SIGN_IS_VISIBLE = 1
 and WRK_PROFILE_SIGN.USER_ID = HRS_SIGNS.SIGN_USER_ID
 and ARC_USERS.EMPNO =  HRS_LETTER_REQUEST.EMPNO
 and HRS_SIGNS.ARC_RECORD_ID=$P{record_id}]]>
