	<queryString>
		<![CDATA[select num,a.TENDERITEMID, a.TENDERITEMNAME, a.ITEMUNITNAME, a.GARD_ACTUAL, a.GARD_DAFTER, a.GARD_ADD, a.GARD_MINUS, a.NOTES
from
(
Select rownum num,DET.TENDERITEMID TENDERITEMID,IT.TENDERITEMNAME TENDERITEMNAME, UN.ITEMUNITNAME ITEMUNITNAME, DET.GARD_ACTUAL GARD_ACTUAL,
DET.GARD_DAFTER GARD_DAFTER,DET.GARD_ADD GARD_ADD,DET.GARD_MINUS GARD_MINUS,DET.NOTES NOTES
from whs_gard_detail det,tdr_tenderitems it,tdr_itemunit un
where DET.GARDID= $P{gardId}  and DET.TENDERITEMID=IT.TENDERITEMID and IT.ITEMUNITID=UN.ITEMUNITID
)A,

(
SELECT  level lev
FROM    dual
CONNECT BY
        level <= 15)B

        where b.lev = a.num (+)

        order by b.lev]]>
	</queryString>
